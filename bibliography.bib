@article{blood_pressure_lowering_treatment_trialists_collaboration_effects_2008-1,
	title = {Effects of different regimens to lower blood pressure on major cardiovascular events in older and younger adults: meta-analysis of randomised trials},
	volume = {336},
	issn = {1756-1833},
	shorttitle = {Effects of different regimens to lower blood pressure on major cardiovascular events in older and younger adults},
	doi = {10.1136/bmj.39548.738368.BE},
	abstract = {OBJECTIVE: To quantify the relative risk reductions achieved with different regimens to lower blood pressure in younger and older adults. DESIGN: Meta-analyses and meta-regression analyses used to compare the effects on the primary outcome between two age groups ({\textbackslash}textless65 v {\textbackslash}textgreater or =65 years). Evidence for an interaction between age and the effects of treatment sought by fitting age as a continuous variable and estimating overall effects across trials. MAIN OUTCOME MEASURES: PRIMARY OUTCOME: total major cardiovascular events. RESULTS: 31 trials, with 190 606 participants, were included. The meta-analyses showed no clear difference between age groups in the effects of lowering blood pressure or any difference between the effects of the drug classes on major cardiovascular events (all P{\textbackslash}textgreater or =0.24). Neither was there any significant interaction between age and treatment when age was fitted as a continuous variable (all P{\textbackslash}textgreater0.09). The meta-regressions also showed no difference in effects between the two age groups for the outcome of major cardiovascular events ({\textbackslash}textless65 v {\textbackslash}textgreater or =65; P=0.38). CONCLUSIONS: Reduction of blood pressure produces benefits in younger ({\textbackslash}textless65 years) and older ({\textbackslash}textgreater or =65 years) adults, with no strong evidence that protection against major vascular events afforded by different drug classes varies substantially with age.},
