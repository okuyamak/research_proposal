	number = {1},
	urldate = {2019-02-06},
	journal = {BMC Public Health},
	author = {Mackenbach, Joreintje D and Rutter, Harry and Compernolle, Sofie and Glonti, Ketevan and Oppert, Jean-Michel and Charreire, Helene and De Bourdeaudhuij, Ilse and Brug, Johannes and Nijpels, Giel and Lakerveld, Jeroen},
