@article{hamanoAssociationChildhoodObesity2017,
	title = {Association between {Childhood} {Obesity} and {Neighbourhood} {Accessibility} to {Fast}-{Food} {Outlets}: {A} {Nationwide} 6-{Year} {Follow}-{Up} {Study} of 944,487 {Children}},
	volume = {10},
	issn = {1662-4025, 1662-4033},
	shorttitle = {Association between {Childhood} {Obesity} and {Neighbourhood} {Accessibility} to {Fast}-{Food} {Outlets}},
	url = {https://www-karger-com.ludwig.lub.lu.se/Article/FullText/481352},
	doi = {10.1159/000481352},
	abstract = {\textbf{\textit{Objectives: }}The aim of this 6-year follow-up study was to examine whether neighbourhood accessibility to fast-food outlets was associated with diagnosed childhood obesity, after adjustment for neighbourhood- and individual-level socio-demographic factors. \textbf{\textit{Methods: }}This 6-year follow-up study comprised 484,677 boys and 459,810 girls aged 0-14 years in Sweden. The follow-up period ran from January 1, 2005, until hospitalisation/out-patient treatment for obesity, death, emigration or the end of the study period on December 31, 2010. Multilevel logistic regression models (individual-level factors at the first level and neighbourhood-level factors at the second level) were used to calculate odds ratios (ORs) with 95\% confidence intervals (95\% CIs). \textbf{\textit{Results: }}We identified 6,968 obesity cases (3,878 boys and 3,090 girls) during the follow-up period. Higher odds of childhood obesity for those living in neighbourhoods with accessibility to fast-food outlets was observed (OR = 1.14, 95\% CI = 1.07-1.22) that remained significant after adjustments (OR = 1.06, 95\% CI = 1.00-1.13). \textbf{\textit{Conclusions: }}This prospective nationwide study showed that the neighbourhood accessibility to fast-food outlets was independently associated with increased odds of diagnosed childhood obesity. This finding implicates that residential environments should be considered when developing health promotion programmes.},
	language = {english},
	number = {6},
	urldate = {2019-07-29},
	journal = {Obesity Facts},
	author = {Hamano, Tsuyoshi and Li, Xinjun and Sundquist, Jan and Sundquist, Kristina},
	year = {2017},
	pmid = {29161708},
	pages = {559--568},
	file = {Full Text PDF:C\:\\Users\\ke5744ok\\Zotero\\storage\\T5K6K5R2\\Hamano et al. - 2017 - Association between Childhood Obesity and Neighbou.pdf:application/pdf;Snapshot:C\:\\Users\\ke5744ok\\Zotero\\storage\\MFKD8UQJ\\481352.html:text/html}
}

