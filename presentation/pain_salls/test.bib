	number = {5},
	urldate = {2019-09-25},
	journal = {American Journal of Health Promotion},
	author = {Koohsari, Mohammad Javad and Kaczynski, Andrew T. and Nakaya, Tomoki and Shibata, Ai and Ishii, Kaori and Yasunaga, Akitomo and Stowe, Ellen W. and Hanibuchi, Tomoya and Oka, Koichiro},
	month = jun,
	year = {2019},
	pages = {764--767},
	file = {SAGE PDF Full Text:C\:\\Users\\ke5744ok\\Zotero\\storage\\I6NYZ32A\\Koohsari et al. - 2019 - Walkable Urban Design Attributes and Japanese Olde.pdf:application/pdf}
}

