	pmid = {17701762},
	keywords = {Sweden, Pain, Anxiety, Immigrants, Self-Reported, Sleeping Problems, Turkish-Born},
	pages = {363--379},
	file = {Snapshot:C\:\\Users\\ke5744ok\\Zotero\\storage\\4DX2FM3Q\\13557850701300673.html:text/html}
